{
	"name": "Vimeo repeat & speed",
	"description": "Repeat & Speed buttons for Vimeo HTML5 video",

	"content_scripts": [ {
		"all_frames": true,
		"js": [ "vimeo.js" ],
		"matches": [ "*://vimeo.com/*", "*://player.vimeo.com/*" ],
		"run_at": "document_end"
	} ],

	"icons": {
		"128": "vimeo128.png",
		"48": "vimeo48.png"
	},
	"version": "1.4",
	"manifest_version": 2
}
